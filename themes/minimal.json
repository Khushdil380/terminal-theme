{
  "name": "Minimal",
  "description": "Clean and minimal theme with subtle colors",
  "version": "1.0.0",
  "prompt_symbol": "symbol:arrow",
  "prompt_symbol_color": "#6b7280",
  "blocks": [
    {
      "type": "directory",
      "enabled": true,
      "foreground": "#374151",
      "background": "#f3f4f6",
      "icon": "[",
      "separator": "",
      "max_length": 60,
      "template": "{{path}}"
    },
    {
      "type": "git-branch",
      "enabled": true,
      "foreground": "#059669",
      "background": "#ecfdf5",
      "icon": "git",
      "separator": "",
      "show_status": true,
      "staged_icon": "+",
      "changes_icon": "~",
      "untracked_icon": "?",
      "template": " {{branch}}{{status}}"
    }
  ]
}